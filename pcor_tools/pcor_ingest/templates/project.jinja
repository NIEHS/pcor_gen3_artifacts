{
  "type": "project",
  "code": "{{ model.code }}",
  "name": "{{ model.name }}",
  "short_name": "{{ model.short_name }}",
  "long_name": "{{model.long_name}}",
  "project_sponsor": [
    {% for entry in model.project_sponsor %}
        "{{ entry }}"
      {{ "," if not loop.last }}
    {% endfor %}
  ],
  "project_sponsor_other": [
    {% for entry in model.project_sponsor_other %}
        "{{ entry }}"
      {{ "," if not loop.last }}
    {% endfor %}
  ],
  "project_sponsor_type": [
    {% for entry in model.project_sponsor_type %}
        "{{ entry }}"
      {{ "," if not loop.last }}
    {% endfor %}
  ],
  "project_sponsor_type_other": [
    {% for entry in model.project_sponsor_type_other %}
        "{{ entry }}"
      {{ "," if not loop.last }}
    {% endfor %}
  ],
  "project_url": "{{ model.project_url }}",
  "description": "{{ model.description }}",
  "dbgap_accession_number": "{{ model.dbgap_accession_number }}"
}