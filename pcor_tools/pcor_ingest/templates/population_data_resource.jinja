{
  "type": "population_data_resource",
  "submitter_id": "{{ pop_data_resource.submitter_id }}",
  "times_available_comment": "{{ pop_data_resource.times_available_comment }}",
  "comments": "{{ pop_data_resource.comments }}",
  "intended_use": "{{ pop_data_resource.intended_use }}",
  "source_name": "{{ pop_data_resource.source_name }}",
  "source_url": "{{ pop_data_resource.source_url }}",
  "update_frequency": "{{ pop_data_resource.update_frequency }}",
  "includes_citizen_collected": "{{ pop_data_resource.includes_citizen_collected }}",
  "has_api": "{{ pop_data_resource.has_api }}",
  "has_visualization_tool": "{{ pop_data_resource.has_visualization_tool }}",

  {% if not pop_data_resource.time_extent_start %}
      "time_extent_start": null,
  {% else %}
      "time_extent_start": "{{ pop_data_resource.time_extent_start }}",
  {% endif %}

   {% if not pop_data_resource.time_extent_end %}
      "time_extent_end": null,
  {% else %}
      "time_extent_end": "{{ pop_data_resource.time_extent_end }}",
  {% endif %}
  "times_available_comment": "{{ pop_data_resource.times_available_comment }}",
  "temporal_resolution": "{{ pop_data_resource.temporal_resolution }}",

  "spatial_coverage": "{{ pop_data_resource.spatial_coverage }}",
  "spatial_resolution": "{{ pop_data_resource.spatial_resolution }}",
  "spatial_bounding_box": "{{ pop_data_resource.spatial_bounding_box }}",
  "geometry_type": "{{ pop_data_resource.geometry_type }}",
  "geometry_source": "{{ pop_data_resource.geometry_source }}",
  "population": [
    {% for pop_entry in pop_data_resource.population %}
        "{{ pop_entry }}"
      {{ "," if not loop.last }}
    {% endfor %}
  ],
  "vulnerable_population": [
    {% for pop_entry in pop_data_resource.vulnerable_population %}
        "{{ pop_entry }}"
      {{ "," if not loop.last }}
    {% endfor %}
  ],
  "exposures":  [
        {% for exposure in pop_data_resource.exposures  %}

            "{{ exposure }}"
          {{ "," if not loop.last }}
        {% endfor %}
                ],
  "outcomes":  [
        {% for outcome in pop_data_resource.outcomes  %}

            "{{ outcome }}"
          {{ "," if not loop.last }}
        {% endfor %}
                ],
  "model_methods": "{{ pop_data_resource.model_methods }}",
  "resources": {
    "node_id": "{{ pop_data_resource.resource_id }}",
    "submitter_id": "{{ pop_data_resource.resource_submitter_id }}"
  }
}